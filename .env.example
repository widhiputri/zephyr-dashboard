# Zephyr Scale API Token (required)
ZEPHYR_API_TOKEN=your-zephyr-scale-api-token

# Zephyr/Jira Base URL (required)
ZEPHYR_BASE_URL=https://your-instance.atlassian.net

# Server port
PORT=3001

# Cache TTLs (seconds)
CACHE_TTL_PROJECTS=3600
CACHE_TTL_METRICS=300

# Rate limit delay between paginated API calls (ms)
PAGINATION_DELAY_MS=200
